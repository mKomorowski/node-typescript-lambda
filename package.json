{
  "name": "node-typescript-lambda",
  "version": "0.0.1",
  "description": "Node typescript template to spin AWS Lambda functions",
  "keywords": [
    "typescript",
    "aws",
    "lambda"
  ],
  "engines": {
    "node": ">= 16.13"
  },
  "scripts": {
    "build": "webpack",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mKomorowski/node-typescript-lambda.git"
  },
  "author": "Michal Komorowski <mkomorowski@windowslive.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mKomorowski/node-typescript-lambda/issues"
  },
  "homepage": "https://github.com/mKomorowski/node-typescript-lambda#readme",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|js)x?$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts}",
      "!src/**/*.d.ts"
    ]
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.91",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7"
  }
}
